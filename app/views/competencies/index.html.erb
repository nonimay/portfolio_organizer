<h1>Listing competencies</h1>

<table>
  <tr>
    <th>Number</th>
    <th>Title</th>
    <th>Section</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @competencies.each do |competency| %>
  <tr>
    <td><%= competency.number %></td>
    <td><%= competency.title %></td>
    <td><%= competency.section %></td>
    <td><%= link_to 'Show', competency %></td>
    <td><%= link_to 'Edit', edit_competency_path(competency) %></td>
    <td><%= link_to 'Destroy', competency, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Competency', new_competency_path %>
